//this is style.styl
html
	body
		table
			display inline-block
			border-top 1px solid black
			border-bottom 1px solid black
			tr
				td
					border-bottom 1px solid black
		.table-controllers
			margin-top 20px
			display table
			vertical-align top
			border 1px solid black
			padding 3px 8px
			.arrows
				display inline-block
				cursor pointer
			.arrows:hover
				color white
				background black
			.page-number
				margin 0px 10px
				display inline-block
			.display
				display inline-block
				margin-left 20px